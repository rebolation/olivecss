{
  "name": "olivecss-vitest",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "test": "run-p test.unit test.babel",
    "test.unit": "run-p test.unit.react test.unit.solid test.unit.svelte test.unit.vue test.babel",
    "test.unit.react": "cross-env FRAMEWORK=react vitest run unit.react --reporter=dot",
    "test.unit.solid": "cross-env FRAMEWORK=solid vitest run unit.solid --reporter=dot",
    "test.unit.svelte": "cross-env FRAMEWORK=svelte vitest run unit.svelte --reporter=dot",
    "test.unit.vue": "cross-env FRAMEWORK=vue vitest run unit.vue --reporter=dot",
    "test.babel": "vitest run babel --reporter=dot",
    "test.babel.react": "vitest run babel.react --reporter=dot",
    "watch": "run-p watch.unit watch.babel",
    "watch.unit": "run-p watch.unit.react watch.unit.solid watch.unit.svelte watch.unit.vue",
    "watch.unit.react": "cross-env FRAMEWORK=react vitest unit.react --reporter=dot",
    "watch.unit.solid": "cross-env FRAMEWORK=solid vitest unit.solid --reporter=dot",
    "watch.unit.svelte": "cross-env FRAMEWORK=svelte vitest unit.svelte --reporter=dot",
    "watch.unit.vue": "cross-env FRAMEWORK=vue vitest unit.vue --reporter=dot",
    "watch.babel": "run-p watch.babel.react",
    "watch.babel.react": "vitest babel.react --reporter=dot"
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "solid-js": "^1.9.5",
    "svelte": "^5.38.2"
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@solidjs/testing-library": "^0.8.10",
    "@sveltejs/vite-plugin-svelte": "^6.1.3",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/svelte": "^5.2.8",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/test-utils": "^2.4.6",
    "babel-plugin-tester": "^12.0.0",
    "cross-env": "^10.0.0",
    "jsdom": "^26.1.0",
    "npm-run-all": "^4.1.5",
    "vite": "^7.1.2",
    "vite-plugin-solid": "^2.10.1",
    "vitest": "^3.2.4",
    "vue": "^3.5.18"
  }
}
